# --- Email (for Outreach Agent & Newsletter Engine) ---
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASSWORD=your-app-password
SMTP_FROM_NAME=James Alford
SMTP_FROM_EMAIL=your-email@gmail.com

# --- Domain & API Keys for Deal Scanner ---
BIZBUYSELL_API_KEY=
FLIPPA_API_KEY=

# --- Tailscale (optional, for remote dashboard access) ---
TAILSCALE_AUTH_KEY=tskey-auth-your-key

# --- Backblaze B2 (backups) ---
B2_KEY_ID=
B2_APPLICATION_KEY=
B2_BUCKET_NAME=jarvis-backups

# --- ChromaDB ---
CHROMA_HOST=localhost
CHROMA_PORT=8000
